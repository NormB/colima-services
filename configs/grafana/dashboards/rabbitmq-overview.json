{
  "title": "RabbitMQ Overview",
  "uid": "rabbitmq-overview",
  "tags": [
    "rabbitmq",
    "messaging",
    "colima"
  ],
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "title": "RabbitMQ Status",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "clamp_max(rabbitmq_erlang_uptime_seconds{job=\"vector\"} > 0, 1)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ]
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Total Messages",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages{job=\"vector\"})",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Message Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_queue_messages_published_total{job=\"vector\"}[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msgs/s"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Connections",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "rabbitmq_connections{job=\"vector\"}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Message Publishing Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_queue_messages_published_total{job=\"vector\"}[5m]))",
          "legendFormat": "Published",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "sum(rate(rabbitmq_channel_messages_delivered_total{job=\"vector\"}[5m]))",
          "legendFormat": "Delivered",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "sum(rate(rabbitmq_channel_messages_acked_total{job=\"vector\"}[5m]))",
          "legendFormat": "Acknowledged",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Queue Messages",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_ready{job=\"vector\"}",
          "legendFormat": "Ready",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "rabbitmq_queue_messages_unacked{job=\"vector\"}",
          "legendFormat": "Unacked",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Consumers",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "rabbitmq_queue_consumers{job=\"vector\"}",
          "legendFormat": "{{queue}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "rabbitmq_process_resident_memory_bytes{job=\"vector\"}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    }
  ]
}