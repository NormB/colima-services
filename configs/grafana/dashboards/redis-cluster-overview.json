{
  "title": "Redis Cluster Overview",
  "uid": "redis-cluster-overview",
  "tags": [
    "redis",
    "cluster",
    "cache",
    "colima"
  ],
  "timezone": "",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "title": "Cluster State",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "min(redis_cluster_state{job=\"vector\"})",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "FAIL",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ]
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Total Keys",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(redis_db_keys{job=\"vector\"})",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Memory Usage",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(redis_memory_used_bytes{job=\"vector\"})",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Commands/sec",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(redis_commands_processed_total{job=\"vector\"}[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Operations Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(redis_commands_total{job=\"vector\",cmd=\"get\"}[5m]))",
          "legendFormat": "GET",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "sum(rate(redis_commands_total{job=\"vector\",cmd=\"set\"}[5m]))",
          "legendFormat": "SET",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "sum(rate(redis_commands_total{job=\"vector\",cmd=\"del\"}[5m]))",
          "legendFormat": "DEL",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Memory Per Node",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes{job=\"vector\"}",
          "legendFormat": "{{addr}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Cluster Slots Distribution",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "redis_cluster_slots_assigned{job=\"vector\"}",
          "format": "table",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    },
    {
      "title": "Connected Clients",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "redis_connected_clients{job=\"vector\"}",
          "legendFormat": "{{addr}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      }
    }
  ]
}